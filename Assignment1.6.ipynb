      "**\n",
      "***\n",
      "****\n",
      "*****\n",
      "****\n",
      "***\n",
      "**\n",
      "*\n",
      "\n"
     ]
    }
   ],
   "source": [
    "n=5\n",
    "for x in [1,n-1]:\n",
    "    for y in range (0,n):\n",
    "        if x !=n-1 :\n",
    "            print (\"*\"*(x+y))\n",
    "        else:\n",
    "            print((\"*\"*(x-y)))\n",
    "    \n",
    "    \n"
